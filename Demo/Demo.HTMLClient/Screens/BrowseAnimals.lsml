  <Application.Methods>
    <ApplicationMethod
      Name="showBrowseAnimals">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="BrowseAnimals" />
      </ApplicationMethod.Attributes>
    </ApplicationMethod>
  </Application.Methods>
</Application>