                  <ControlPropertySource
                    Property="Microsoft.LightSwitch.MobileWeb:RootControl/Properties[Tap]">
                    <ScreenExpressionTree>
                      <ChainExpression>
                        <CallExpression
                          Target="!module/Methods[showAddAnimalInfoSmart]">
                          <CallExpression.Attributes>
                            <CreateNewEntity
                              EntityType="AnimalInfo"
                              TargetParameter="!module/Methods[showAddAnimalInfoSmart]/Parameters[AnimalInfo]" />
                          </CallExpression.Attributes>
                          <ArgumentExpression
                            Parameter="Animal">
                            <ChainExpression>
