                <ContentItem.PropertySources>
                  <ControlPropertySource
                    Property="Microsoft.LightSwitch.MobileWeb:RootControl/Properties[Tap]">
                    <ScreenExpressionTree>
                      <ChainExpression>
                        <CallExpression
                          Target="!module/Methods[showWeasel]">
                          <ArgumentExpression
                            Parameter="myData">
                            <ChainExpression>
                              <MemberExpression
                                Member="BrowseAnimals/Properties[AnimalsSet]" />
                              <MemberExpression
                                Member="selectedItem" />
                              <MemberExpression
                                Member="Name" />
                            </ChainExpression>
                          </ArgumentExpression>
                        </CallExpression>
                      </ChainExpression>
                    </ScreenExpressionTree>
                  </ControlPropertySource>
                </ContentItem.PropertySources>
              </ContentItem>
              <ContentItem
                Name="ShowWeaselCodeWay"
                Kind="Command">
                <ContentItem.Attributes>
                  <DisplayName
                    Value="Show Weasel (Code Way)" />
